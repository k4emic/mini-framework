<?xml version="1.0"?>
<transfers xmlns="spryker:databuilder-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd">
    <transfer name="UserData" strict="true">
        <property name="uuid" dataBuilderRule="uuid()"/>
        <property name="username" dataBuilderRule="word()"/>
    </transfer>

    <transfer name="Greeting" strict="true">
        <property name="greet" dataBuilderRule="word()"/>
    </transfer>

</transfers>
